{
  "name": "@galadrim/galacrypt",
  "version": "2.3.0",
  "type": "module",
  "main": "./bin/index.js",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf bin && tsc && chmod +x ./bin/index.js",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "start": "npm run build && ./bin/index.js",
    "test": "rm -rf bin && vitest run",
    "test:watch": "rm -rf bin && vitest",
    "deploy": "npm run build && npm publish --access public"
  },
  "devDependencies": {
    "@types/node": "^20.11.25",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "typescript": "~5.3.3",
    "vitest": "^4.0.10"
  },
  "bugs": {
    "url": "https://github.com/galadrimteam/galacrypt/issues"
  },
  "homepage": "https://github.com/galadrimteam/galacrypt#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/galadrimteam/galacrypt.git"
  },
  "files": [
    "bin",
    "README.md",
    "package.json"
  ],
  "bin": "./bin/index.js"
}
